{
  "name": "@quodsi/shared",
  "version": "1.0.0",
  "private": true,
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "test": "jest",
    "test:update-snapshots": "ts-node --project tsconfig.test.json tests/serialization/generateFixtures.ts",
    "test:update-single-snapshot": "ts-node --project tsconfig.test.json tests/serialization/generateFixtures.ts",
    "test:verify-snapshots": "jest ModelSerializer.snapshot.test.ts",
    "test:verify-single-snapshot": "cross-env TEST_MODELS=%npm_config_model% jest ModelSerializer.snapshot.test.ts"
  },
  "devDependencies": {
    "@jest/types": "^29.6.3",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.17.19",
    "cross-env": "^7.0.3",
    "jest": "^29.7.0",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "axios": "^1.7.9"
  }
}